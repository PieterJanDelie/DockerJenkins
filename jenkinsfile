pipeline {

    agent any
    
    stages {
      stage('Clonerepo') {
          steps {
            echo 'git clone'
          }
     }
      stage('Build') {
          steps {
            echo 'this is the build stage'
          }
     }
     stage('Test') {
          steps {
            echo 'this is the test stage'
          }
     }
     stage('Deploy') {
          steps {
            echo 'this is the deploy stage'
          }
     }
}
